{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "lahmfactory"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"OnpremSQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnpremSQLDB'"
		},
		"OnpremSQLDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnpremSQLDB'"
		},
		"Source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Source'"
		},
		"OnpremSQLDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Administrator"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"Source_connectionString": {
						"value": "[parameters('Source_connectionString')]"
					},
					"OnpremSQLDB_connectionString": {
						"value": "[parameters('OnpremSQLDB_connectionString')]"
					},
					"OnpremSQLDB_password": {
						"value": "[parameters('OnpremSQLDB_password')]"
					},
					"OnpremSQLDB_properties_typeProperties_userName": {
						"value": "[parameters('OnpremSQLDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}